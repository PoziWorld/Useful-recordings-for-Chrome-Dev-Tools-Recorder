{
  "title": "Amazon → Subscribe & Save → Cancel subscription",
  "steps": [
    {
      "type": "setViewport",
      "width": 1024,
      "height": 768,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "https://www.amazon.com/auto-deliveries/subscriptionList",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://www.amazon.com/auto-deliveries/subscriptionList",
          "title": ""
        }
      ]
    },
    {
      "type": "click",
      "selectors": [
        [
          ".subscription-hover-state-container:nth-of-type(1)"
        ]
      ],
      "target": "main",
      "offsetX": 20,
      "offsetY": 20
    },
    {
      "type": "click",
      "selectors": [
        [
          "#a-popover-content-1 .t-action-type-CANCEL"
        ]
      ],
      "target": "main",
      "offsetX": 20,
      "offsetY": 20
    },
    {
      "type": "click",
      "selectors": [
        [
          "#sns-cancellation-dropdown"
        ]
      ],
      "target": "main",
      "offsetX": 20,
      "offsetY": 20
    },
    {
      "type": "click",
      "selectors": [
        [
          "[data-value='{\"stringVal\":\"SnS_MYD_SnsCancelReason_sns_cancel_reason_stopped_using\"}']"
        ],
        [
          "#sns-cancellation-dropdown_1"
        ]
      ],
      "target": "main",
      "offsetX": 20,
      "offsetY": 20
    },
    {
      "type": "click",
      "selectors": [
        [
          "#confirmCancelLink [aria-labelledby=\"confirmCancelLink-announce\"]"
        ]
      ],
      "target": "main",
      "offsetX": 20,
      "offsetY": 20
    }
  ]
}
